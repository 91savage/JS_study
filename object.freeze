const user1={
    name: 'Savage',
    address: {
        city : 'Seoul'
    }
};

// Object.assign 은 완전한 Deep copy 지원 하지않음.

const user2 = Object.assign({},user1, {name:'jinstarr'});

console.log(user1.name);
console.log(user2.name);

Object.freeze(user1);

user1.name = 'Savage91'; // 무시됨. 프리즈 되었기 때문

console.log(user1);
console.log(Object.isFrozen(user1));